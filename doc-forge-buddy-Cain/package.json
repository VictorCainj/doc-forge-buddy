{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "node server.js",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build --mode analyze",
    "build:production": "PERFORMANCE=true vite build --mode production",
    "build:performance": "npm run build:production",
    "build:bundle": "npm run build:analyze && node scripts/performance-monitor.js",
    "build:optimized": "npm run lint:fix && npm run type-check && npm run build:performance",
    "start": "node server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:watch": "vitest --config vitest.integration.config.ts",
    "test:integration:coverage": "vitest run --config vitest.integration.config.ts --coverage",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "npx playwright show-report",
    "test:e2e:codegen": "npx playwright codegen",
    "test:e2e:install": "npx playwright install --with-deps",
    "test:e2e:update": "npx playwright install --with-deps",
    "test:e2e:clean": "node scripts/clean-test-results.js",
    "test:e2e:setup-data": "node scripts/generate-test-data.js",
    "test:e2e:all-browsers": "playwright test --project=chromium --project=firefox --project=webkit",
    "coverage:report": "vitest --coverage && open coverage/index.html || echo 'Coverage report generated at coverage/index.html'",
    "coverage:threshold": "vitest run --coverage && npm run validate-coverage",
    "coverage:reports": "node scripts/generate-coverage-reports.js",
    "coverage:pr-comment": "node scripts/generate-pr-comment.js",
    "validate-coverage": "node scripts/validate-coverage.js",
    "test:lighthouse": "lhci autorun",
    "test:performance": "node scripts/performance-monitor.js",
    "test:budgets": "npm run build:production && npm run test:performance",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --config=lighthouserc.js",
    "lighthouse:mobile": "lhci autorun --preset=mobile",
    "lighthouse:desktop": "lhci autorun --preset=desktop",
    "optimize": "npm run lint:fix && npm run type-check && npm run build",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "security:full-audit": "npm audit --audit-level high",
    "security:check-snyk": "snyk test",
    "security:snyk-monitor": "snyk monitor",
    "security:update": "npm outdated && npm update",
    "security:scan": "npm run security:audit && npm run security:check-snyk",
    "security:report": "npm run security:scan && node scripts/security-scanner.js",
    "prepare": "husky",
    "postinstall": "husky install",
    "quality-gates": "npm run test:ci && npm run validate-coverage && npm run lint",
    "ci:performance": "npm run build:production && npm run test:performance && npm run test:lighthouse",
    "ci:bundle-analysis": "npm run build:analyze && echo 'Bundle analysis available at: dist/bundle-analysis.html'",
    "monitor:performance": "npm run build:performance && node scripts/performance-monitor.js",
    "validate:budgets": "node scripts/performance-monitor.js",
    "report:performance": "npm run build:production && node scripts/performance-monitor.js && echo 'Reports available at: dist/performance-report.*'",
    "ci:full": "npm run lint && npm run type-check && npm run test:coverage && npm run test:e2e && npm run coverage:threshold",
    "validate:quality-gates": "node scripts/validate-quality-gates.js",
    "finalize:quality-gates": "node scripts/finalize-quality-gates.js",
    "csp:validate": "npx tsx scripts/csp-validator.ts",
    "csp:test": "npm run csp:validate -- http://localhost:3000",
    "csp:prod": "npm run csp:validate -- https://production-domain.com",
    "csp:report": "npm run csp:validate -- http://localhost:3000 csp-full-report.json",
    "csp:server": "npx tsx server.ts",
    "csp:dev": "npm run dev & npm run csp:server",
    "husky:setup": "node scripts/setup-husky.js",
    "husky:check": "node scripts/setup-husky.js --check",
    "commit:validate": "node scripts/validate-commit-msg.js",
    "quality:validate": "node scripts/validate-quality.js",
    "quality:precommit": "node scripts/validate-quality.js",
    "git:hooks": "npx husky install && npx husky add .husky/pre-commit && npx husky add .husky/commit-msg && npx husky add .husky/pre-push",
    "setup:complete": "npm install && npm run husky:setup",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "storybook build && serve storybook-static",
    "storybook:test": "test-storybook --watch=false",
    "storybook:update-snapshots": "test-storybook --update-snapshots --watch=false",
    "build:storybook": "storybook build -o storybook-static",
    "deploy:storybook": "npm run build:storybook && npm run deploy:static",
    "storybook:build:ci": "storybook build -o storybook-static --quiet",
    "storybook:analyze": "npm run build:storybook && npx http-server storybook-static -p 6006 -o",
    "storybook:lint": "eslint . --ext .stories.ts,.stories.tsx",
    "storybook:check": "npm run storybook:lint && npm run storybook:test && npm run storybook:build",
    "storybook:docs": "storybook build --docs && serve storybook-static -p 6006"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "tsc --noEmit"
    ],
    "*.{test.ts,test.tsx,spec.ts,spec.tsx,test.js,test.jsx,spec.js,spec.jsx}": [
      "vitest run --reporter=verbose --reporter=dot"
    ],
    "src/**/*.{ts,tsx,js,jsx}": [
      "vitest run --reporter=verbose --reporter=dot"
    ],
    "*.{json,md,css,scss,sass,less}": [
      "prettier --write"
    ],
    "*.{html,xml}": [
      "prettier --write"
    ],
    "*.{yml,yaml}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  },
